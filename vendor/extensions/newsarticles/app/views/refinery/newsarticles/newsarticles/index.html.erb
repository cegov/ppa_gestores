<div class="not-so-fluid">
	<div class="container-fluid">
		<div class="row-fluid">
			<div class="">
			  <ul id="newsarticles">
			    <% @newsarticles.each do |newsarticle| %>
			    	<% if newsarticle.is_published? %>
				      <li>
				        <%= link_to newsarticle.headline, refinery.newsarticles_newsarticle_path(newsarticle) %>
				      </li>
				    <% end %>
			    <% end %>
			  </ul>
			</div>
		</div>
	</div>
</div>